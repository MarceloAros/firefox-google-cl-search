{
	"name": "Google CL Search",
	"description": "Add google.cl to your search engines (including search suggestions from google.cl).",
	"version": "0.1",
	"manifest_version": 2,
	"applications":{
		"gecko": {
			"id": "firefox-googleclsearch@marceloaros.cl",
			"strict_min_version": "64.0"
		}
	},
	"developer": {
		"name": "Marcelo Aros",
		"url": "https://github.com/MarceloAros/"
	},
	"icons": {
		"128": "favicon.png"
	},
	"web_accessible_resources": [
		"favicon.png"
	],
  "chrome_settings_overrides": {
		"search_provider": {
			"name": "Google CL",
			"search_url": "https://www.google.cl/search?q={searchTerms}",
			"suggest_url": "https://www.google.cl/complete/search?client=firefox&hl=es-cl&q={searchTerms}"
		}
	}
}
